// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id        Int      @id @default(autoincrement())
  topic     String
  payload   Json
  createdAt DateTime @default(now())

  @@index([topic])
  @@index([createdAt])
}
